{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GUI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/MockGUI/bin/Debug/net7.0/MockGUI.dll",
            "args": ["--golem", "modules"],
            "cwd": "${workspaceFolder}",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                // "Logging__LogLevel__Default": "Debug",
                // "Logging__LogLevel__Microsoft": "Information"
                "YA_NET_RELAY_HOST": "127.0.0.1:17464"
            }
        },
        {
            "name": "CLI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/FacadeApp/bin/Debug/net7.0/FacadeApp.dll",
            "args": ["--golem", "modules/golem", "--data_dir", "modules/golem-data"],
            "cwd": "${workspaceFolder}",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                // "Logging__LogLevel__Default": "Debug",
                // "Logging__LogLevel__Microsoft": "Information"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
    ],
}
